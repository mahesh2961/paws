    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent" android:tag="layout/fragment_category_images_0" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/image_list"
            android:tag="binding_1"                                                        
